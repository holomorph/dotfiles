application/octet-stream; echo %s "can be anything..."; copiousoutput;
text/html; elinks -dump-charset %{charset} -dump %s; copiousoutput;

application/pdf; zathura %s;
application/postscript; zathura %s;

application/x-tex; vim %s;
application/x-latex; vim %s;

image/*; feh -. %s;
audio/*; mplayer %s; copiousoutput;
video/*; mplayer %s; copiousoutput;

application/vnd.openxmlformats-officedocument.wordprocessingml.document; docx2txt %s; nametemplate=%s.docx; copiousoutput;
application/vnd.openxmlformats-officedocument.spreadsheetml.sheet; xlsx2csv %s | sed -f ~/.mutt/xlsx.sed | column -ts; copiousoutput;
application/vnd.ms-excel; xls2csv %s | sed -f ~/.mutt/xls.sed | column -ts; copiousoutput;
application/msword; antiword -f %s; copiousoutput;
