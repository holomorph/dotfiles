# ~/.xbindkeysrc

# mpd control
# "ncmpcpp prev"
#     m:0x8 + c:34
#     Alt + bracketleft
# "ncmpcpp next"
#     m:0x8 + c:35
#     Alt + bracketright
# "ncmpcpp toggle"
#     m:0x8 + c:51
#     Alt + backslash

# volume control
# "dwmvolume toggle"
#     m:0x0 + c:121
#     XF86AudioMute
# "dwmvolume decrease"
#     m:0x0 + c:122
#     XF86AudioLowerVolume
# "dwmvolume increase"
#     m:0x0 + c:123
#     XF86AudioRaiseVolume

# brightness control
# "bset -d 2"
#     m:0x0 + c:232
#     XF86MonBrightnessDown
# "bset -i 2"
#     m:0x0 + c:233
#     XF86MonBrightnessUp

# external display
"vex"
    m:0x0 + c:235
    XF86Display
